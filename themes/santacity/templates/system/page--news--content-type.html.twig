{% set container = theme.settings.fluid_container ? 'container-fluid' : 'container' %}
{% include 'header.html.twig' %}
<div class="container-fluid back">
	<div class="event_in_bnr" style="background: url( {{ file_url(node.field_news_image.entity.uri.value) }}) no-repeat;"></div>
	<div class="container">
		<div class="row">
				<div class="col-md-12 wh box_article_page">
					{% if page.breadcrumb %}
					{% block breadcrumb %}
					{{ page.breadcrumb }}
					{% endblock %}
					{% endif %}
					
					{% if page.header %}
					{% block header %}
					{{ page.header }}
					{% endblock %}
					{% endif %}
					<div class="arti_date news-cotent">
						<span >{{ node.field_start_date.value|date("F, d Y") }}</span>
						<span class="city-cat">Category: {{ node.field_news_category.value }}</span>
						{% if page.social_share %}
						<div class="pull-right">
							{{ page.social_share }}
						</div>
						{% endif %}
						<div class="col-sm-12">
							{{ node.body.value|raw }}
						</div>
					</div>
				</div>
	{{ page.content.recent_article }}
	{{ comments }}
			</div>
	</div>	
</div>	
{% include 'footer.html.twig' %}
